<h2>Register</h2>
<%= form_with url: register_3_url, method: :patch, data: {turbo: false} do |f| %>

  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email", value: resource.email, disabled: true %>
  </div>

  <div class="field">
    <%= f.label :full_name%>
    <%= f.text_field :full_name%>
  </div>

  <div class="field">
    <%= f.label :password %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "new-password" %>
  </div>

  <div class="field">
    <%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation, autocomplete: "new-password" %>
  </div>

  <div class="field">
    <%= f.label :curriculum_vitate %><br />
    <%= f.file_field :curriculum_vitate, accept:".doc, .pdf, .xls, .xlsx, .zip"%>
  </div>
  
  

  <div class="actions">
    <%= f.submit "Register" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>


